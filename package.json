{
  "name": "fracturedjsonjs",
  "version": "2.1.69",
  "description": "JSON formatter that produces human-readable but fairly compact output",
  "main": "dist/fracturedjson.js",
  "types": "dist/fracturedjson.d.ts",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf dist/* && mkdirp dist",
    "compile": "tsc -p ./tsconfig.json",
    "test": "tsc --noEmit -p ./tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/j-brooke/FracturedJsonJs.git"
  },
  "keywords": [
    "JSON",
    "formatter",
    "pretty printer",
    "beautifier",
    "stringify"
  ],
  "author": "j-brooke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/j-brooke/FracturedJsonJs/issues"
  },
  "homepage": "https://j-brooke.github.io/FracturedJson/",
  "devDependencies": {
    "@types/node": "12.7.1",
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2",
    "typescript": "3.5.3"
  },
  "dependencies": {
    "eastasianwidth": "^0.2.0"
  },
  "devDependenciesComments": {
    "removed": {
      "@types/node": "^16.3.1",
      "typescript": "^4.3.5"
    }
  },
  "dependenciesComments": {
    "removed": {}
  }
}
